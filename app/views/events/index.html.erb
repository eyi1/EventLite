<h3>Filter Events:</h3>
 
<%= form_tag({}, method: "get") do %>
    <%= select_tag "date", options_for_select(["Upcoming", "Past"]), include_blank: true %>
    <%= submit_tag "Search" %>
  <% end %>

<ul>
<% @events.each do |event| %>
    <li><%= link_to event.title, event_path(event) %></li>
<% end %>
</ul>
<br>
<p>Want to Create an Event?</p>
<p><%= button_to 'GO', @my_url, method: :get%>
<%= link_to 'My Account', user_path(current_user), class: "btn btn-primary btn-light" %>